<article>
    <img src="{{countryFlag}}" alt="{{countryName}}">
    <h2>{{countryName}}</h2>
    <ul>
        <li>
            <h3>Population</h3>
            <p>{{countryPopulation}}</p>
        </li>
        <li>
            <h3>Capital</h3>
            <p>{{countryCapital}}</p>
        </li>
        <li>
            <h3>Languages</h3>
            <p>{{countryLanguages}}</p>
        </li>
    </ul>
</article>